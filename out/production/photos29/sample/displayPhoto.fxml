<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DisplayPhotoController">
   <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="dateLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="80" styleClass="dateLabel" text="Date" textAlignment="CENTER" wrapText="true" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="20.0">
               <font>
                  <Font name="Avenir Next Bold" size="13.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></Label>
            <ImageView fx:id="photoImageView" fitHeight="160.0" fitWidth="200" layoutX="52.0" layoutY="55.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="170.0" AnchorPane.leftAnchor="115.0" AnchorPane.rightAnchor="115.0" AnchorPane.topAnchor="100.0" />
            <VBox fx:id="middleVBox" alignment="CENTER" prefHeight="100" prefWidth="389.0" spacing="10.0" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="captionLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="29.0" prefWidth="133.0" styleClass="displayLabel" text="Caption">
                           <HBox.margin>
                              <Insets left="130.0" />
                           </HBox.margin>
                           <font>
                              <Font name="Droid Serif" size="13.0" />
                           </font></Label>
                        <Button fx:id="recaptionButton" mnemonicParsing="false" onAction="#recaptionPressed" prefHeight="29.0" prefWidth="87.0" styleClass="secondaryButton1" text="Recaption">
                           <HBox.margin>
                              <Insets left="15.0" />
                           </HBox.margin>
                           <font>
                              <Font size="11.0" />
                           </font></Button>
                        <VBox prefHeight="29.0" prefWidth="45.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></VBox>
                     </children>
                  </HBox>
                  <ListView fx:id="tagsListView" prefHeight="200.0" prefWidth="70.0">
                     <VBox.margin>
                        <Insets left="115.0" right="115.0" top="2.0" />
                     </VBox.margin></ListView>
                  <HBox prefHeight="64.0" prefWidth="430.0">
                     <children>
                        <Button fx:id="addTagButton" mnemonicParsing="false" onAction="#addTagPressed" styleClass="createButton" text="Add Tag">
                           <HBox.margin>
                              <Insets left="120.0" />
                           </HBox.margin>
                           <font>
                              <Font size="11.0" />
                           </font></Button>
                        <Button fx:id="deleteTagButton" mnemonicParsing="false" onAction="#deleteTagPressed" styleClass="deleteButton" text="Delete Tag">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                           <font>
                              <Font size="11.0" />
                           </font></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <right>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="logOutButton" layoutY="20.0" mnemonicParsing="false" onAction="#logOutPressed" prefHeight="31.0" prefWidth="82.0" styleClass="logoutButton" text="Log Out" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0" />
            <Button fx:id="editButton" layoutY="351.0" mnemonicParsing="false" onAction="#editPressed" styleClass="secondaryButton2" text="Edit Photo" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />
         </children>
      </AnchorPane>
   </right>
   <left>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="20.0" spacing="5.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0">
               <children>
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deletePressed" prefHeight="30.0" prefWidth="86.0" styleClass="deleteButton" text="Delete All">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#savePressed" prefHeight="29.0" prefWidth="87.0" styleClass="createButton" text="Save Edits">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backPressed" prefHeight="29.0" prefWidth="79.0" styleClass="backButton" text="Back" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0" />
         </children>
      </AnchorPane>
   </left>
   <stylesheets>
      <URL value="@buttons.css" />
   </stylesheets>
</BorderPane>
