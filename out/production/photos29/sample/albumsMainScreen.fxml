<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AlbumsMainScreenController">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="albumsLabel" text="Albums" textFill="#612961">
               <font>
                  <Font name="AppleMyungjo Regular" size="48.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin></Label>
            <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#logOutPressed" text="Log Out">
               <HBox.margin>
                  <Insets left="320.0" top="30.0" />
               </HBox.margin></Button>
         </children></HBox>
   </top>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="editAlbumButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="57.0" text="Edit">
               <HBox.margin>
                  <Insets left="30.0" top="60.0" />
               </HBox.margin></Button>
            <Button fx:id="renameAlbumButton" mnemonicParsing="false" text="Rename">
               <HBox.margin>
                  <Insets left="20.0" top="60.0" />
               </HBox.margin></Button>
            <Button fx:id="deleteAlbumButton" mnemonicParsing="false" text="Delete">
               <HBox.margin>
                  <Insets left="20.0" top="60.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="createAlbumButton" mnemonicParsing="false" text="Create">
               <HBox.margin>
                  <Insets left="260.0" top="60.0" />
               </HBox.margin>
            </Button>
         </children></HBox>
   </bottom>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="searchAlbumsTextField" prefHeight="27.0" prefWidth="534.0" promptText="Search Albums">
               <VBox.margin>
                  <Insets left="20.0" right="200.0" />
               </VBox.margin>
            </TextField>
            <ListView fx:id="albumsListView" prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="15.0" />
               </VBox.margin>
            </ListView>
         </children></VBox>
   </center>
</BorderPane>
