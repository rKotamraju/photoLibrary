<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StockPhotosController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <left>
            <VBox prefHeight="300.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="dogImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#dogClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                     </VBox.margin></ImageView>
                  <ImageView fx:id="catImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#catClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                     </VBox.margin></ImageView>
               </children></VBox>
         </left>
         <right>
            <VBox prefHeight="300.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="flowerImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#flowerClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="10.0" right="20.0" top="20.0" />
                     </VBox.margin></ImageView>
                  <ImageView fx:id="chocolateImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#chocolateClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="10.0" right="20.0" top="20.0" />
                     </VBox.margin></ImageView>
               </children></VBox>
         </right>
         <center>
            <VBox prefHeight="300.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="happyBabyImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#babyClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="15.0" top="20.0" />
                     </VBox.margin></ImageView>
                  <ImageView fx:id="familyImageView" fitHeight="130.0" fitWidth="160.0" onMouseClicked="#familyClicked" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="15.0" top="20.0" />
                     </VBox.margin></ImageView>
               </children>
            </VBox>
         </center>
         <top>
            <HBox prefHeight="68.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" styleClass="backButton">
                     <HBox.margin>
                        <Insets left="20.0" top="25.0" />
                     </HBox.margin></Button>
                  <Label text="Choose Stock Photo" textFill="#8267d3">
                     <font>
                        <Font name="Avenir Next Condensed Bold" size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="130.0" top="15.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </top>
      </BorderPane>
   </children>

   <stylesheets>
      <URL value="@buttons.css" />
   </stylesheets>

</AnchorPane>
